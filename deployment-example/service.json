{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "mph-sftp-gcsfuse-test",
        "labels": {
            "app": "mph-sftp-gcsfuse-test"
        },
        "annotations": {
            "external-dns.alpha.kubernetes.io/hostname": "mph-sftp-gcsfuse-test.sftp.dev.redimapl.com",
            "external-dns.alpha.kubernetes.io/ttl": "60"
        }
    },
    "spec": {
        "type": "LoadBalancer",
        "externalTrafficPolicy": "Local",
        "ports": [
            {
                "port": 22,
                "targetPort": 22
            }
        ],
        "selector": {
            "app": "mph-sftp-gcsfuse-test"
        }
    }
}